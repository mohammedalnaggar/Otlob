<h1>New Group Member</h1>

<% for user in @users %>
  <div class="user">
    <p>
      <strong><%=h user.email %></strong>
      <%= link_to "Add", group_group_members_path(params[:group_id],:user_id => user), :method => :post %>
      <div class="clear"></div>
    </p>
  </div>
<% end %>


<%= link_to 'Cancel', group_group_members_path %>

